{
  "name": "myProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build": "npm run tsc",
    "clean:android": "cd android/app && rm -rf build",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "ios:11pm": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios:11p": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "ios:12": "react-native run-ios --simulator=\"iPhone 12\"",
    "ios:8": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:device": "npx react-native run-ios --device",
    "ios": "react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "lint": "eslint .",
    "pod:install": "cd ios && pod install",
    "remove:ios:build": "cd ios && rm -rf build",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/datetimepicker": "^7.0.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "babel-plugin-dotenv-import": "^3.0.1",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "^0.71.6",
    "react-native-dropdown-select-list": "^2.0.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-touch-id": "^4.4.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "^5.2.1",
    "react-native-video-player": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
